set residue9  BEYN   !
set residue10 2OXT
set residue11 3OXT
set residue12 nc2
set residue13 nc3
set residue14 nh4
set residue15 PRF

!set residue15 B85
!set residue16 B85C
set seed1   1 N5 1 C1  1 N2 ! TEAZ
set seed2   1 C1  1 N2  1 N3 ! MTEA
set seed3   1 C1  1 N2  1 N3 ! ETEA
