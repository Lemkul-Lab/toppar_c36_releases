    set patchname = @{patch_@@pind}

    ! Sanity check
    if resi1 .eq. @resi2 stop    ! Invalid residues
    if patchname .eq. NSTP stop  ! NSTP is for N-terminal patch only
    
    ! force resi1 .le. resi2
    if resi1 .gt. @resi2 then
        set resi3 = @resi1
        set resi1 = @resi2
        set resi2 = @resi3
    endif
    
    ! NSTP must be used if resi1 == 1, NTER otherwise
    if resi1 .eq. 1 then
        set first = NSTP
    else
        set first = NTER
    endif
    
    ! DIBM / DIBP require cysteine, others require alanine
    set typetest = @patchname
    trim typetest to 3
    if typetest .eq. DIB then
        set restype = CYS
    else
        set restype = ALA
    endif
    
    read sequence card
    * dummy peptide sequence
    *
     7
     @restype @restype @restype @restype @restype @restype @restype 
    
    generate PROA first @first last CTER setup warn 
    autogen angles dihedrals
    
    patch @patchname PROA @resi1 PROA @resi2 setup warn
    autogen angles dihedrals

    !ic generate
    ic param
    ic seed PROA 1 N PROA 1 CA PROA 1 C
    ic build

goto skip_staple_dihe
    
    ! dihedral restraint is only required for (R)META3-7
    set typetest = @patchname
    trim typetest to 5
    if typetest .ne. RMETA then
        trim typetest to 4
        if typetest .ne. META goto skip_staple_dihe
    endif
    
    cons fix sele .not. (resid @resi1 .or. resid @resi2) end
    
    ! carbon atom attached to CM1
    define junk sele type C* .and. .not. type CM* -
                     .and. .BONDED. (resid @resi1 .and. type CM1) end
    set CM0 = ?seltype
    define junk sele type C* .and. .not. type CM* -
                     .and. .BONDED. (resid @resi1 .and. type CM2) end
    set CM3 = ?seltype
    
    ! maintain cis double bond (C1==C2)
    cons dihe force 250 min 0.0 width 0.0 -
              PROA @resi1 @CM0 -
              PROA @resi1  CM1 -
              PROA @resi1  CM2 -
              PROA @resi1 @CM3
    
label skip_staple_dihe
    
    ENERGY
    
    delete atom select all end

    incr pind
    if pind .le. @npatches goto do_test

